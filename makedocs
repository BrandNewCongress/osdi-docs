# parameters
PARAM='--toc -f markdown_github'

# make main protocol doc
pandoc -o output/protocol.docx $PARAM docheader.md README.md aep.md people.md questions.md interactions.md

# make scenarios doc
pandoc -o output/scenarios.docx $PARAM docheader.md scenarios/scenarios.md

#make charter doc
pandoc -o output/charter.docx $PARAM docheader.md charter.md

#make review guide doc
pandoc -o output/review.docx $PARAM docheader.md review_guide.md